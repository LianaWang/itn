####################fusion#########################
#######res5c, 2048########
layer {
    name: "up_res5c"
    type: "Deconvolution"
    bottom: "res5c"
    top: "up_res5c"
    convolution_param {
      num_output: 2048
      group: 2048
      kernel_size: 8 
      stride: 4
      pad: 2
      weight_filler: { 
          type: "bilinear" 
      }
      bias_term: false 
    }
    param { lr_mult: 0 decay_mult: 0}
}
layer {
  name: "out_res5c"
  type: "Convolution"
  bottom: "up_res5c"
  top: "out_res5c"
  param { lr_mult: 1.0 }
  param { lr_mult: 2.0 }
  convolution_param {
    num_output: 512
    kernel_size: 3 pad: 1 stride: 1
    weight_filler { type: "gaussian" std: 0.01 }
    bias_filler { type: "constant" value: 0 }
  }
}
layer {
  name : "crop_res5c"
  type: "Crop"
  bottom: "out_res5c"
  bottom: "res3d"
  top: "crop_res5c"
}
###res4f, 1024###########
layer {
    name: "up_res4f"
    type: "Deconvolution"
    bottom: "res4f"
    top: "up_res4f"
    convolution_param {
      num_output: 1024
      group: 1024
      kernel_size: 4
      stride: 2
      pad: 1
      weight_filler: { 
          type: "bilinear" 
      }
      bias_term: false 
    }
    param { lr_mult: 0 decay_mult: 0}
}
layer {
  name: "out_res4f"
  type: "Convolution"
  bottom: "up_res4f"
  top: "out_res4f"
  param { lr_mult: 1.0 }
  param { lr_mult: 2.0 }
  convolution_param {
    num_output: 512
    kernel_size: 3 pad: 1 stride: 1
    weight_filler { type: "gaussian" std: 0.01 }
    bias_filler { type: "constant" value: 0 }
  }
}
layer {
  name : "crop_res4f"
  type: "Crop"
  bottom: "out_res4f"
  bottom: "res3d"
  top: "crop_res4f"
}
###res3d, 1024###########
layer {
  name: "out_res3d"
  type: "Convolution"
  bottom: "res3d"
  top: "out_res3d"
  param { lr_mult: 1.0 }
  param { lr_mult: 2.0 }
  convolution_param {
    num_output: 512
    kernel_size: 3 pad: 1 stride: 1
    weight_filler { type: "gaussian" std: 0.01 }
    bias_filler { type: "constant" value: 0 }
  }
}
######concatenate res3d, res4f, res5c#################
layer{
  name: "concat_345"
  type: "Concat"
  bottom: "out_res3d"
  bottom: "crop_res4f"
  bottom: "crop_res5c"
  top: "out_concat"
}
layer {
  name: "fusion"
  type: "Convolution"
  bottom: "out_concat"
  top: "fusion"
  param { lr_mult: 1.0 }
  param { lr_mult: 2.0 }
  convolution_param {
    num_output: 1024
    kernel_size: 3 pad: 1 stride: 1
    weight_filler { type: "gaussian" std: 0.01 }
    bias_filler { type: "constant" value: 0 }
  }
}

layer {
	bottom: "fusion"
	top: "fusion"
	name: "fusion_relu"
	type: "ReLU"
}

#========= RPN ============
layer {
  name: "fusion_matrix"
  type: "Convolution"
  bottom: "fusion"
  top: "fusion_matrix"
  param {
    lr_mult: 0.01
  }
  param {
    lr_mult: 0.02
  }
  convolution_param {
    num_output: 8
    kernel_size: 3
    dilation: 2
    pad: 2
    stride: 1
    engine: CAFFE #do not use CUDNN for dilation 
    weight_filler{
      type:"constant"
      value: 0
    }
    bias_filler{
      type:"constant"
      value: 0
    }
  }
}

layer {
  name: "fusion_offset"
  type: "TransformOffset"
  bottom: "fusion_matrix"
  top: "fusion_offset"
  transform_offset_param {
    num_output: 98
  }
}

layer {
  name: "rpn_conv"
  type: "DeformableConvolution"
  bottom: "fusion"
  bottom: "fusion_offset"
  top: "rpn/output"
  param { lr_mult: 1.0 }
  param { lr_mult: 2.0 }
  convolution_param {
    num_output: 512
    kernel_size: 7
    pad: 3 
    stride: 1
    weight_filler { type: "gaussian" std: 0.01 }
    bias_filler { type: "constant" value: 0 }
  }
}
layer {
  name: "rpn_relu"
  type: "ReLU"
  bottom: "rpn/output"
  top: "rpn/output"
}

layer {
  name: "rpncls_score"
  type: "Convolution"
  bottom: "rpn/output"
  top: "rpncls_score"
  param { lr_mult: 1.0 }
  param { lr_mult: 2.0 }
  convolution_param {
    num_output: 2   # 2(bg/fg) * 1(anchors)
    kernel_size: 1 pad: 0 stride: 1
    weight_filler { type: "gaussian" std: 0.01 }
    bias_filler { type: "constant" value: 0 }
  }
}


layer {
  name: "rpnbbox_pred"
  type: "Convolution"
  bottom: "rpn/output"
  top: "rpnbbox_offset"
  param { lr_mult: 1.0 }
  param { lr_mult: 2.0 }
  convolution_param {
    num_output: 8   # 5 * 1(anchors)
    kernel_size: 1 pad: 0 stride: 1
    weight_filler { type: "gaussian" std: 0.01 }
    bias_filler { type: "constant" value: 0 }
  }
}

layer {
  name: "anchor_offset"
  type: "TransformAnchor"
  bottom: "fusion_matrix"
  top: "anchor_offset"
  transform_anchor_param {
    stride: 8
    kernel_size: 7
  }
}

layer{
  name: "combine_offset"
  type: "Eltwise"
  bottom: "rpnbbox_offset"
  bottom: "anchor_offset"
  top: "rpnbbox_pred"
}



layer {
   bottom: "rpncls_score"
   top: "rpn_cls_score_reshape"
   name: "rpn_cls_score_reshape"
   type: "Reshape"
   reshape_param { shape { dim: 0 dim: 2 dim: -1 dim: 0 } }
}

####################training######################
layer {
  name: 'rpn-data'
  type: 'Python'
  bottom: 'rpncls_score'
  bottom: 'gt_boxes'
  bottom: 'im_info'
  bottom: 'data'
  top: 'rpn_labels'
  top: 'rpn_bbox_targets'
  top: 'rpn_bbox_inside_weights'
  top: 'rpn_bbox_outside_weights'
  top: 'homography'
  python_param {
    module: 'rpn.anchor_target_layer'
    layer: 'AnchorTargetLayer'
    param_str: "{'feat_stride': 8, 'kernel_size': 7}"
  }
}

layer {
  name: "rpn_loss_cls"
  type: "SoftmaxWithLoss"
  bottom: "rpn_cls_score_reshape"
  bottom: "rpn_labels"
  propagate_down: 1
  propagate_down: 0
  top: "rpn_cls_loss"
  loss_weight: 1
  loss_param {
    ignore_label: -1
    normalize: true
  }
}

layer {
  name: "rpn_loss_bbox"
  type: "SmoothL1Loss"
  bottom: "rpnbbox_pred"
  bottom: "rpn_bbox_targets"
  bottom: 'rpn_bbox_inside_weights'
  bottom: 'rpn_bbox_outside_weights'
  top: "rpn_loss_bbox"
  loss_weight: 0.1
  smooth_l1_loss_param { sigma: 3.0 }
}

layer {
  name: "rpn_loss_matrix"
  type: "SmoothL1Loss"
  bottom: "fusion_matrix"
  bottom: "homography"
  bottom: 'rpn_bbox_inside_weights'
  bottom: 'rpn_bbox_outside_weights'
  top: "rpn_loss_matrix"
  loss_weight: 0.01
  smooth_l1_loss_param { sigma: 3.0 }
}

